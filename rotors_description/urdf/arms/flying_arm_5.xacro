<?xml version="1.0" encoding="utf-8"?>
<robot name="flying_arm_5" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:property name="PI" value="3.1415926535897931" />
  <xacro:property name="effort_limit_arm_bl_link1" value="1" />
  <xacro:property name="velocity_limit_arm_bl_link1" value="10000" />

  <xacro:property name="effort_limit_arm_link1_link2" value="1" />
  <xacro:property name="velocity_limit_arm_link1_link2" value="10000" />

  <xacro:property name="effort_limit_arm_link2_link3" value="0.3" />
  <xacro:property name="velocity_limit_arm_link2_link3" value="10000" />

  <xacro:property name="effort_limit_arm_link3_link4" value="0.3" />
  <xacro:property name="velocity_limit_arm_link3_link4" value="10000" />

  <xacro:property name="effort_limit_arm_link4_link5" value="0.3" />
  <xacro:property name="velocity_limit_arm_link4_link5" value="10000" />

  <xacro:property name="arm_namespace" value="flying_arm_5" />

  <xacro:macro name="flying_arm_base_macro" params="namespace base_link origin_xyz origin_rpy">

    <joint name="${arm_namespace}/j_base_link" type="fixed">
      <origin xyz="${origin_xyz}" rpy="${origin_rpy}" />
      <parent link="${namespace}/${base_link}" />
      <child link="${arm_namespace}/base_link" />
    </joint>

    <link name="${arm_namespace}/base_link">
      <inertial>
        <origin
          xyz="5.4471E-05 0.00055544 0.0045971"
          rpy="0 0 0" />
        <mass value="0.27973" />
        <inertia
          ixx="0.00010284"
          ixy="5.704E-10"
          ixz="7.6592E-09"
          iyy="7.9509E-05"
          iyz="5.7105E-10"
          izz="9.2151E-05" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://rotors_description/meshes/arms/flying_arm5/base_link.dae" />
        </geometry>
        <material name="">
          <color rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
    </link>

    <gazebo reference="${arm_namespace}/base_link">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>

    <joint name="${arm_namespace}/j_base_link_link_1" type="revolute">
      <origin xyz="0 0 0" rpy="1.5708 1.5708 0" />
      <parent link="${arm_namespace}/base_link" />
      <child link="${arm_namespace}/link_1" />
      <axis xyz="0 0 1" />
      <limit lower="${- PI / 2 - 0.1}" upper="${ PI / 2 + 0.1}" effort="${effort_limit_arm_bl_link1}" velocity="${velocity_limit_arm_bl_link1}" />
      <dynamics damping="0.1" friction="0.1" />
    </joint>

    <link name="${arm_namespace}/link_1">
      <inertial>
        <origin xyz="0.097289 5.2467E-05 -0.012147" rpy="0 0 0" />
        <mass value="0.12284" />
        <inertia ixx="2.3846E-05" ixy="-5.0883E-13" ixz="3.5276E-06" iyy="0.00021635" iyz="-1.2916E-12" izz="0.00023624" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://rotors_description/meshes/arms/flying_arm5/link_1.dae" />
        </geometry>
        <material name="">
          <color rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
    </link>


    <gazebo reference="${namespace}/link_1">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>


    <link name="${arm_namespace}/link_2">
      <inertial>
        <origin xyz="0.043238 2.4308E-05 -0.0087926" rpy="0 0 0" />
        <mass value="0.11474" />
        <inertia ixx="1.174E-05" ixy="1.3294E-09" ixz="-2.0499E-06" iyy="4.7747E-05" iyz="-1.6102E-08" izz="5.2344E-05" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://rotors_description/meshes/arms/flying_arm5/link_2.dae" />
        </geometry>
        <material name="">
          <color rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
    </link>

    <gazebo reference="${arm_namespace}/link_2">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>

    <joint name="${arm_namespace}/j_link_1_link_2" type="revolute">
      <origin xyz="0.132 0 0" rpy="0 0 0" />
      <parent link="${arm_namespace}/link_1" />
      <child link="${arm_namespace}/link_2" />
      <axis xyz="0 0 1" />
      <limit lower="${- PI / 2 - 0.1}" upper="${ PI / 2 + 0.1}" effort="${effort_limit_arm_link1_link2}" velocity="${velocity_limit_arm_link1_link2}" />
      <dynamics damping="0.1" friction="0.1" />
    </joint>

    <link name="${arm_namespace}/link_3">
      <inertial>
        <origin xyz="0.00020827 -0.0031223 -0.011966" rpy="0 0 0" />
        <mass value="0.068772" />
        <inertia ixx="6.8293E-06" ixy="4.2309E-08" ixz="1.548E-09" iyy="7.2432E-06" iyz="1.6138E-08" izz="7.1043E-06" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://rotors_description/meshes/arms/flying_arm5/link_3.dae" />
        </geometry>
        <material name="">
          <color rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
    </link>

    <gazebo reference="${arm_namespace}/link_3">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>

    <joint name="${arm_namespace}/j_link_2_link_3" type="revolute">
      <origin xyz="0.075 0 0" rpy="0 0 0" />
      <parent link="${arm_namespace}/link_2" />
      <child link="${arm_namespace}/link_3" />
      <axis xyz="0 0 1" />
      <limit lower="${- PI / 2 - 0.1}" upper="${ PI / 2 + 0.1}" effort="${effort_limit_arm_link2_link3}" velocity="${velocity_limit_arm_link2_link3}" />
      <dynamics damping="0.1" friction="0.1" />
    </joint>

    <link name="${arm_namespace}/link_4">
      <inertial>
        <origin xyz="0.026125 2.9724E-09 0.0016875" rpy="0 0 0" />
        <mass value="0.05914" />
        <inertia ixx="2.2712E-05" ixy="2.3256E-12" ixz="5.2509E-09" iyy="2.472E-05" iyz="3.3225E-09" izz="1.0444E-05" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://rotors_description/meshes/arms/flying_arm5/link_4.dae" />
        </geometry>
        <material name="">
          <color rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
    </link>

    <gazebo reference="${arm_namespace}/link_4">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>

    <joint name="${arm_namespace}/j_link_3_link_4" type="revolute">
      <origin xyz="0 0 -0.011925" rpy="1.5708 0 0" />
      <parent link="${arm_namespace}/link_3" />
      <child link="${arm_namespace}/link_4" />
      <axis xyz="0 0 1" />
      <limit lower="${- PI / 2 - 0.1}" upper="${ PI / 2 + 0.1}" effort="${effort_limit_arm_link3_link4}" velocity="${velocity_limit_arm_link3_link4}" />
      <dynamics damping="0.1" friction="0.1" />
    </joint>

    <link name="${arm_namespace}/link_5">
      <inertial>
        <origin xyz="1.7347E-18 -1.0246E-17 0.025817" rpy="0 0 0" />
        <mass value="0.021791" />
        <inertia ixx="7.9107E-06" ixy="1.6653E-13" ixz="2.9904E-21" iyy="1.5446E-05" iyz="-1.1963E-21" izz="1.0818E-05" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://rotors_description/meshes/arms/flying_arm5/link_5.dae" />
        </geometry>
        <material name="">
          <color rgba="0.75294 0.75294 0.75294 1" />
        </material>
      </visual>
    </link>

    <gazebo reference="${arm_namespace}/link_5">
      <mu1>0.9</mu1>
      <mu2>0.9</mu2>
    </gazebo>

    <joint name="${arm_namespace}/j_link_4_link_5" type="revolute">
      <origin xyz="0.034 0 -0.00025" rpy="1.5708 0 1.5708" />
      <parent link="${arm_namespace}/link_4" />
      <child link="${arm_namespace}/link_5" />
      <axis xyz="0 0 1" />
      <limit lower="${- PI /2 - 0.1}" upper="${ PI /2 + 0.1}" effort="${effort_limit_arm_link4_link5}" velocity="${velocity_limit_arm_link4_link5}" />
    </joint>

    <xacro:include filename="$(find rotors_description)/urdf/component_snippets.xacro" />

    <gazebo>
      <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
        <robotNamespace>${namespace}</robotNamespace>
      </plugin>
    </gazebo>

    <xacro:transmission_interface
      name="transmission1"
      joint_name="${arm_namespace}/j_base_link_link_1"
      actuator_name="motor1" />

    <xacro:transmission_interface
      name="transmission2"
      joint_name="${arm_namespace}/j_link_1_link_2"
      actuator_name="motor2" />

    <xacro:transmission_interface
      name="transmission3"
      joint_name="${arm_namespace}/j_link_2_link_3"
      actuator_name="motor3" />

    <xacro:transmission_interface
      name="transmission4"
      joint_name="${arm_namespace}/j_link_3_link_4"
      actuator_name="motor4" />

    <xacro:transmission_interface
      name="transmission5"
      joint_name="${arm_namespace}/j_link_4_link_5"
      actuator_name="motor5" />
  </xacro:macro>
</robot>